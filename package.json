{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "start": "bunx rork start -p fwkirhovhuv8bp6hssbjv --tunnel",
    "start-web": "bunx rork start -p fwkirhovhuv8bp6hssbjv --web --tunnel",
    "start-web-dev": "DEBUG=expo* bunx rork start -p fwkirhovhuv8bp6hssbjv --web --tunnel",
    "deps:check": "node scripts/dependency-manager.js check",
    "deps:update": "node scripts/dependency-manager.js update",
    "deps:fix": "node scripts/dependency-manager.js fix",
    "deps:outdated": "node scripts/dependency-manager.js outdated",
    "deps:peers": "node scripts/dependency-manager.js peers",
    "deps:audit": "node scripts/dependency-manager.js audit",
    "deps:clean": "rm -rf node_modules package-lock.json && npm install",
    "postinstall": "npm run deps:peers",
    "deps:auto": "node scripts/dependency-manager.js --auto-schedule",
    "test": "jest",
    "test:ci": "jest --ci --coverage --reporters=default --maxWorkers=50%",
    "test:ci:retries": "jest --ci --coverage --reporters=default --retryTimes=2 --maxWorkers=50%",
    "pretypecheck": "node scripts/pretypecheck-clean.js",
    "typecheck": "npm run pretypecheck && tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "lint:summary": "node scripts/lint-summary.js",
    "contrast:check": "node scripts/check-contrast.js",
    "lint:baseline": "node scripts/generate-eslint-baseline.js",
    "lint:ci": "node scripts/lint-ci.js",
    "codemod:colors": "node scripts/codemod-color-literals.js",
    "lint:fix": "eslint . --fix --max-warnings=0",
    "danger:ci": "danger ci",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.85.3",
    "expo": "^53.0.20",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.4",
    "expo-font": "~13.3.0",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-location": "~18.1.5",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.7",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.6",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.475.0",
    "nativewind": "^4.1.23",
    "react": "^19.0.0",
    "react-dom": "19.0.0",
    "react-native": "^0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "zod": "^4.1.5",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@expo/ngrok": "^4.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "danger": "^12.3.0",
    "eslint": "8.57.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.10",
    "ts-jest": "^29.2.5",
    "typescript": "~5.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "overrides": {
    "lucide-react-native": {
      "react": "$react",
      "react-native": "$react-native"
    }
  },
  "private": true,
  "trustedDependencies": [
    "core-js"
  ]
}
